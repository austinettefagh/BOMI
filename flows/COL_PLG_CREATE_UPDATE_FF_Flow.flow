            </conditions>
            <conditions>
                <leftValueReference>$Record.LLC_BI__Collateral__r.LLC_BI__Collateral_Type__r.LLC_BI__Collateral_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>312</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.LLC_BI__Collateral__r.LLC_BI__Collateral_Type__r.LLC_BI__Collateral_Code__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>313</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CheckMarketValueForCrops</targetReference>
            </connector>
            <label>Collateral Code Check</label>
