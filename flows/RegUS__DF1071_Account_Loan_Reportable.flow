    <decisions>
        <description>This decision checks whether the relationship&apos;s annual revenue meets the threshold for DF1071.</description>
        <name>Is_Annual_Revenue_less_than_5_million</name>
        <label>Is Annual Revenue less than 5 million?</label>
        <locationX>424</locationX>
        <locationY>323</locationY>
        <defaultConnector>
            <targetReference>Set_ChangeReportableTo_to_False</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Greater than 5 Million</defaultConnectorLabel>
        <rules>
            <name>Less_than_or_Equal_to_5_Million</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.AnnualRevenue</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>5000000.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_ChangeReportableTo_to_True</targetReference>
            </connector>
            <label>Less than or Equal to 5 Million</label>
        </rules>
    </decisions>
