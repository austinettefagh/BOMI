    <decisions>
        <description>If the loan has a loan detail, save it for later to update.  If it does not have a loan detail, create one and update it later in the flow.</description>
        <name>Does_the_Loan_have_a_Loan_Detail</name>
        <label>Does the Loan have a Loan Detail?</label>
        <locationX>578</locationX>
        <locationY>863</locationY>
        <defaultConnector>
            <targetReference>Create_Loan_Detail_Details</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes_Loan_Detail_Exists</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Loop_through_Loans_with_HMDA_False.LLC_BI__Loan_Detail__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Add_to_Loan_Detail_Collection</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
