}

.slds-input-has-icon_right .slds-input__icon,
.slds-input-has-icon--right .slds-input__icon {
	right: 0.45rem;
}

.slds-form-element__control .slds-icon.slds-icon_x-small,
.slds-form-element__control .slds-icon--x-small {
	width: 16px;
	height: 16px;
	line-height: 1;
}

/* nds-form styles */
.slds .slds-size_1-of-1 {
	width: 100%;
}

@media (min-width: 48em) {
	.slds .slds-medium-size_1-of-2 {
		width: 50%;
	}
}

nds-form .slds-col.slds-size_1-of-1.slds-medium-size_1-of-2 {
	min-height: 42px;
}

.slds-form-element .slds-checkbox [type='checkbox'] + .slds-checkbox--faux,
.slds-form-element .slds-checkbox [type='checkbox'] + .slds-checkbox__label .slds-checkbox--faux,
.slds-form-element .slds-checkbox [type='checkbox'] + .slds-checkbox__label .slds-checkbox_faux,
.slds-form-element .slds-checkbox [type='checkbox'] + .slds-checkbox_faux,
.slds-form-element .slds-checkbox [type='checkbox'] ~ .slds-checkbox--faux,
.slds-form-element .slds-checkbox [type='checkbox'] ~ .slds-checkbox_faux {
	margin-right: 0.5rem;
}

.slds-checkbox .slds-checkbox--faux,
.slds-checkbox .slds-checkbox_faux {
	width: 1.5rem;
	height: 1.5rem;
	display: inline-block;
	position: relative;
	vertical-align: middle;
	border: 1px solid #dddbda;
	border-radius: 0.125rem;
	background: #fff;
	transition: border 0.1s linear, background-color 0.1s linear;
}

.slds-checkbox .slds-checkbox__label .slds-form-element__label {
	display: inline;
	vertical-align: middle;
	font-size: 0.8125rem;
}

.slds-form-element__label {
	overflow-wrap: break-word;
	word-wrap: break-word;
	hyphens: auto;
	display: inline-block;
	color: #3e3e3c;
	font-size: 0.75rem;
	padding-right: 0.5rem;
	padding-top: 0.25rem;
	margin-bottom: 0.125rem;
}

.slds-checkbox [type='checkbox']:checked + .slds-checkbox--faux:after,
.slds-checkbox [type='checkbox']:checked + .slds-checkbox__label .slds-checkbox--faux:after,
.slds-checkbox [type='checkbox']:checked + .slds-checkbox__label .slds-checkbox_faux:after,
.slds-checkbox [type='checkbox']:checked + .slds-checkbox_faux:after,
.slds-checkbox [type='checkbox']:checked ~ .slds-checkbox--faux:after,
.slds-checkbox [type='checkbox']:checked ~ .slds-checkbox_faux:after {
	display: block;
	content: '';
	height: 0.45rem;
	width: 0.8rem;
	position: absolute;
	top: 42%;
	left: 50%;
	transform: translate3d(-50%, -50%, 0) rotate(-45deg);
	border-bottom: 2px solid #0070d2;
	border-left: 2px solid #0070d2;
}

.slds-checkbox--toggle .slds-checkbox--faux {
	background-color: #0070d2;
	transition-property: background-color;
	transition-duration: 0.2s;
	transition-timing-function: cubic-bezier(0.75, 0, 0.08, 1);
	transition-delay: 0s;
}

.slds .slds-checkbox--toggle [type='checkbox']:checked ~ .slds-checkbox--faux:after {
	display: block;
	content: ' ';
	height: 15px;
	width: 7px;
	position: absolute;
	top: 0.25rem;
	left: 1.5rem;
	transform: rotate(45deg);
	border-bottom: 2px solid #ffffff;
	border-right: 2px solid #ffffff;
	border-radius: 0;
	background-color: transparent;
}

.slds .slds-checkbox--toggle [type='checkbox']:checked ~ .slds-checkbox--faux:hover {
	background-color: #005fb2;
}

.slds .slds-button--stretch {
	width: 100%;
}

.slds .slds-button--justify-left {
	text-align: left;
	justify-content: left;
}

.slds .dropdown .dropdown-menu a.slds-button:hover,
.slds .dropdown .dropdown-menu a.slds-button:focus {
	outline: 0;
	border-color: #1589ee;
	box-shadow: 0 0 3x #0070d2;
}

.slds .dropdown .dropdown-menu:hover a.slds-button:focus {
	outline: 0;
	border-color: transparent;
	box-shadow: none;
	background: none;
}

.slds .dropdown .dropdown-menu:hover a.slds-button:hover {
	outline: 0;
	border-color: #1589ee;
	box-shadow: 0 0 3x #0070d2;
	background: #fff;
}

.slds .slds-notify--toast button.slds-button.slds-button--icon-inverse.slds-notify__close:focus {
	outline: solid 1px white;
	box-shadow: 0 0 3px white;
}

/* nds-picklist-overrides */
