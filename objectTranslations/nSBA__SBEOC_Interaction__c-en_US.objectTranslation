        <label><!-- Context ID --></label>
        <name>nSBA__Context_Id__c</name>
    </fields>
    <fields>
        <help><!-- This automatically populated required field displays an ID that refers to all events and activities in the scope of this transaction. --></help>
        <label><!-- Correlation ID --></label>
        <name>nSBA__Correlation_Id__c</name>
    </fields>
    <fields>
        <help><!-- This system-populated required field displays the interaction type associated with this interaction record. --></help>
        <label><!-- Interaction Type --></label>
        <name>nSBA__Interaction_Type__c</name>
        <picklistValues>
            <masterLabel>SBEOC</masterLabel>
            <translation><!-- SBEOC --></translation>
        </picklistValues>
    </fields>
    <fields>
        <help><!-- This automatically populated field displays the success status of the transaction. --></help>
        <label><!-- Is Successful --></label>
        <name>nSBA__Is_Successful__c</name>
    </fields>
    <fields>
        <help><!-- This automatically populated field displays the most recent integration message (such as errors or last recorded success state). --></help>
        <label><!-- Message --></label>
        <name>nSBA__Message__c</name>
    </fields>
    <fields>
        <help><!-- This automatically populated field displays the parent record to which this transaction is associated. --></help>
