        <name>nSBA__Parent_Interaction__c</name>
        <relationshipLabel><!-- SBEOC Interactions --></relationshipLabel>
    </fields>
    <fields>
        <help><!-- This system-populated required field displays the status to which this transaction was set when created. --></help>
        <label><!-- Status --></label>
        <name>nSBA__Status__c</name>
    </fields>
    <fields>
        <help><!-- This automatically populated required field displays the fully qualified class name of the vendor provider for this transaction. --></help>
