    <validationRules>
        <fullName>VR_EXISTING_CHANGE_MEMO</fullName>
        <active>false</active>
        <description>Check for the existing Change Memo</description>
        <errorConditionFormula>AND(
$User.No_Validation__c = false,
OR((ISPICKVAL(LLC_BI__Approval_Status__c, &apos;Not Approved&apos;)), (ISPICKVAL(LLC_BI__Approval_Status__c, &apos;In Review&apos;)))
)</errorConditionFormula>
        <errorDisplayField>LLC_BI__Approval_Required__c</errorDisplayField>
        <errorMessage>Only one change memo can be in progress at a time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Memo_LoanAmount_Increase_1L_10Percent</fullName>
        <active>true</active>
        <errorConditionFormula>CF_ProposedIncreaseAmount10__c &gt;=1</errorConditionFormula>
        <errorMessage>Please ensure proposed loan amount increase is less than 25% of the original loan amount. If it is greater than 25%, please create a new loan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_Memo_LoanAmount_Increased_5K_5Percent</fullName>
        <active>true</active>
        <errorConditionFormula>CF_ProposedIncreaseAmount5__c &gt;=1</errorConditionFormula>
        <errorMessage>Please ensure the proposed loan amount increase is within 5% of the originally approved loan amount or $25,000, whichever is greater.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>VR_NewMoney_should_be_same_as_LoanAmount</fullName>
        <active>true</active>
        <errorConditionFormula>CF_ProposedNewMoney__c&gt;=1</errorConditionFormula>
        <errorMessage>Please check that New Money has been updated to reflect the proposed increase in the Loan Amount.</errorMessage>
    </validationRules>
