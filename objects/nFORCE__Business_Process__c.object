        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>nFORCE__Log_Level_To_Capture__c</fullName>
        <deprecated>false</deprecated>
        <description>Select the appropriate log level to capture for this process. - ERROR, WARN, INFO, DEBUG, FINE, FINER, FINEST.</description>
        <externalId>false</externalId>
        <inlineHelpText>Using this optional field, administrators can select a log level to capture for the process to communicate what occurred during the execution. The options available are ERROR, WARN, INFO, DEBUG, FINE, FINER, FINEST.</inlineHelpText>
        <label>Log Level To Capture</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Error</fullName>
                    <default>true</default>
                    <label>Error</label>
                </value>
                <value>
                    <fullName>Warning</fullName>
                    <default>false</default>
                    <label>Warning</label>
                </value>
                <value>
                    <fullName>Info</fullName>
                    <default>false</default>
                    <label>Info</label>
                </value>
                <value>
                    <fullName>Debug</fullName>
                    <default>false</default>
                    <label>Debug</label>
                </value>
                <value>
                    <fullName>Fine</fullName>
                    <default>false</default>
                    <label>Fine</label>
                </value>
                <value>
                    <fullName>Finer</fullName>
                    <default>false</default>
                    <label>Finer</label>
                </value>
                <value>
                    <fullName>Finest</fullName>
                    <default>false</default>
                    <label>Finest</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
