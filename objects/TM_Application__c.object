    <validationRules>
        <fullName>Support_Specialist_can_not_be_null</fullName>
        <active>true</active>
        <errorConditionFormula>!ISNEW()&amp;&amp; ISBLANK(TM_Support_Specialist__c )</errorConditionFormula>
        <errorDisplayField>TM_Support_Specialist__c</errorDisplayField>
        <errorMessage>Support Specialist can&apos;t be null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TM_This_Application_cant_be_edited</fullName>
        <active>true</active>
        <errorConditionFormula>AND($Profile.Name == &apos;Treasury Sales Officer&apos;, NOT(ISPICKVAL(PRIORVALUE(TM_Application_Stage__c),&quot;Review&quot;)), NOT(ISPICKVAL(PRIORVALUE(TM_Application_Stage__c),&quot;Data Input&quot;)), NOT(ISPICKVAL(PRIORVALUE(TM_Application_Stage__c),&quot;Underwriting&quot;)))</errorConditionFormula>
        <errorMessage>This Application can’t be edited anymore as it’s at/after Signature Required stage.</errorMessage>
    </validationRules>
