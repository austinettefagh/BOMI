        <description>This field is used to capture the exposure value</description>
        <externalId>false</externalId>
        <formula>CF_Requested_ACH_Company_Limit__c *  CF_File_Frequency_Value__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
