    <validationRules>
        <fullName>CVR_ACH_Company_Maintenance_Validation</fullName>
        <active>true</active>
        <description>This VR fires when a new ACH company is added or edited when Next Treasury Review Date is less than TODAY.</description>
        <errorConditionFormula>OR(
AND(
NOT(ISNEW()),
ISCHANGED(CF_Requested_ACH_Company_Limit__c ),
CF_Treasury_Service__r.CF_Next_Treasury_Review_Date__c &lt; TODAY(),
CF_Treasury_Service__r.LLC_BI__Is_Under_Maintenance__c = true
),
AND(
ISNEW(),
CF_Treasury_Service__r.CF_Next_Treasury_Review_Date__c &lt; TODAY(),
CF_Treasury_Service__r.LLC_BI__Is_Under_Maintenance__c = true
))</errorConditionFormula>
        <errorMessage>A current Treasury Review must be completed in order to update product limits.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TM_This_ACH_Companies_cant_be_edited</fullName>
        <active>true</active>
        <errorConditionFormula>$Profile.Name == &apos;Treasury Sales Officer&apos; &amp;&amp; TEXT(TM_Treasury_Application_Product__r.TM_Treasury_Application__r.TM_Application_Stage__c) &lt;&gt; &apos;Review&apos; &amp;&amp; TEXT(TM_Treasury_Application_Product__r.TM_Treasury_Application__r.TM_Application_Stage__c) &lt;&gt; &apos;Data Input&apos; &amp;&amp; TEXT(TM_Treasury_Application_Product__r.TM_Treasury_Application__r.TM_Application_Stage__c) &lt;&gt; &apos;Underwriting&apos;</errorConditionFormula>
        <errorMessage>This ACH Companies can’t be edited anymore as it’s at/after Signature Required stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Transaction_Type_Other_need_Description</fullName>
        <active>true</active>
        <description>This VR fires when Transaction type is selected as Other and Other Transaction Type Description field left blank</description>
        <errorConditionFormula>AND( 
ISPICKVAL( TM_Transaction_Type__c , &apos;Other&apos;),
CF_Other_Description__c  = &apos;&apos;
)</errorConditionFormula>
        <errorMessage>Other Transaction Type Description need to be filled when Transaction type is Other</errorMessage>
    </validationRules>
