    <fields>
        <fullName>CF_File_Frequency_Value__c</fullName>
        <description>Used to formulate File Frequency Value based on File Frequency input.</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(TM_Frequency__c,&apos;Daily&apos;),3,
IF(ISPICKVAL(TM_Frequency__c,&apos;Weekly/bi-Weekly&apos;),2,
IF(ISPICKVAL(TM_Frequency__c,&apos;Monthly/bi-Monthly&apos;),1.5,
IF(ISPICKVAL(TM_Frequency__c,&apos;Quarterly/annually&apos;),1.25,
0))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>File Frequency Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
