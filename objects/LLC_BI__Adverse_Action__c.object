    <validationRules>
        <fullName>AA_VR_Decline_Reason_Rule</fullName>
        <active>true</active>
        <description>You must select at least (2) reasons for loan declination when declining a loan</description>
        <errorConditionFormula>AND( 
$User.No_Validation__c = False,
OR( 

AND( 
NOT(ISPICKVAL(LLC_BI__Decline_Reason_1__c , &apos;None&apos;)), 
ISPICKVAL(LLC_BI__Decline_Reason_2__c , &apos;None&apos;), 
ISPICKVAL(LLC_BI__Decline_Reason_3__c , &apos;None&apos;), 
ISPICKVAL(LLC_BI__Decline_Reason_4__c , &apos;None&apos;),
ISNULL(LLC_BI__Comments__c)), 

AND( 
NOT(ISPICKVAL(LLC_BI__Decline_Reason_2__c , &apos;None&apos;)), 
ISPICKVAL(LLC_BI__Decline_Reason_1__c , &apos;None&apos;), 
ISPICKVAL(LLC_BI__Decline_Reason_3__c , &apos;None&apos;), 
ISPICKVAL(LLC_BI__Decline_Reason_4__c , &apos;None&apos;),
ISNULL(LLC_BI__Comments__c)), 

AND( 
NOT(ISPICKVAL(LLC_BI__Decline_Reason_3__c , &apos;None&apos;)), 
ISPICKVAL(LLC_BI__Decline_Reason_1__c , &apos;None&apos;), 
ISPICKVAL(LLC_BI__Decline_Reason_2__c , &apos;None&apos;), 
ISPICKVAL(LLC_BI__Decline_Reason_4__c , &apos;None&apos;),
ISNULL(LLC_BI__Comments__c)), 

AND( 
NOT(ISPICKVAL(LLC_BI__Decline_Reason_4__c , &apos;None&apos;)), 
ISPICKVAL(LLC_BI__Decline_Reason_1__c , &apos;None&apos;), 
ISPICKVAL(LLC_BI__Decline_Reason_2__c , &apos;None&apos;), 
ISPICKVAL(LLC_BI__Decline_Reason_3__c , &apos;None&apos;),
ISNULL(LLC_BI__Comments__c))))</errorConditionFormula>
        <errorMessage>You must select at least (2) reasons OR 1 reason and comments for loan declination when declining a loan</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AA_VR_HMDA_Entity_Compliance_Rule</fullName>
        <active>true</active>
        <description>Complete the HMDA Entity Compliance before initiating adverse action</description>
        <errorConditionFormula>AND(
$User.No_Validation__c = False,
LLC_BI__Loan__r.LLC_BI__Is_Modification__c = false,
LLC_BI__Loan__r.LLC_BI__isRenewal__c = false,
LLC_BI__Loan__r.LLC_BI__HMDA_Reportable__c = true,
OR(LLC_BI__Loan__r.Is_Borrower_an_Individual__c = true,
LLC_BI__Loan__r.CF_Is_Co_Borrower_Individual__c = true),
LLC_BI__Loan__r.Entity_Involvement__r.CF_Entity_Compliance_Created__c = false
)</errorConditionFormula>
        <errorMessage>Please go back and complete the HMDA Entity Compliance before initiating adverse action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AA_VR_HMDA_Loan_Compliance_Rule</fullName>
        <active>true</active>
        <description>Complete the HMDA Loan Compliance before initiating adverse action</description>
        <errorConditionFormula>AND(
  $User.No_Validation__c = False,
  LLC_BI__Loan__r.LLC_BI__Is_Modification__c = false,
  LLC_BI__Loan__r.LLC_BI__isRenewal__c = false,
  LLC_BI__Loan__r.LLC_BI__HMDA_Reportable__c = true,
  LLC_BI__Loan__r.CF_Loan_Compliance_Created__c = false
)</errorConditionFormula>
        <errorMessage>Please go back and complete the HMDA Loan Compliance before initiating adverse action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AA_VR_HMDA_Rule</fullName>
        <active>true</active>
        <description>Complete the HMDA questionnaire before initiating adverse action</description>
        <errorConditionFormula>AND(
  $User.No_Validation__c = False,
  LLC_BI__Loan__r.LLC_BI__Is_Modification__c = false,
  LLC_BI__Loan__r.LLC_BI__isRenewal__c = false,
  LLC_BI__Loan__r.CF_HMDA_QUESTIONNAIRE_ANSWERED__c = false
)</errorConditionFormula>
        <errorMessage>Please go back and complete the HMDA Eligibility Questionnaire before initiating adverse action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AA_VR_Withdraw_Reason_Rule</fullName>
