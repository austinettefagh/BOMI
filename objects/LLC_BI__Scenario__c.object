    <validationRules>
        <fullName>CVR_RestrictConversionTo_Single_Proforma</fullName>
        <active>true</active>
        <description>This VR is to ensure only one Proforma is converted to Treasury Service per Opportunity.</description>
        <errorConditionFormula>AND(
ISPICKVAL( LLC_BI__Opportunity__r.CF_Has_Converted_Proforma__c , &quot;Yes&quot;) ,
ISCHANGED( LLC_BI__Is_Converted__c ),
LLC_BI__Is_Converted__c = true 
)</errorConditionFormula>
        <errorMessage>You cannot convert this Proforma since another Proforma has already been converted on this Opportunity.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CVR_Scenario_record_Locked</fullName>
        <active>true</active>
        <description>This validation rule is to ensure that users are not making any changes to the record once it was submitted for approval.</description>
        <errorConditionFormula>AND( 
NOT( ISBLANK(Date_Submitted_For_Approval__c) ),

OR(
ISCHANGED(Name),
ISCHANGED(CF_Deposit_Account_Type__c),
ISCHANGED(LLC_BI__Average_Ledger_Balance__c),
ISCHANGED(LLC_BI__Average_Unavailable__c),
ISCHANGED(CF_First_Billing_Month__c),
ISCHANGED(CF_Free_Rent_Period_In_Days__c),
ISCHANGED(CF_Reserve_Requirement_Rate__c),
ISCHANGED(CF_Earnings_Credit_Rate__c),
ISCHANGED(CF_Competitor_Bank__c),
ISCHANGED(CF_Exception_Pricing_Expiration_Date__c),
ISCHANGED(CF_Comments__c)
)
)</errorConditionFormula>
        <errorMessage>This record is already in Approval &amp; hence can&apos;t be edited.</errorMessage>
    </validationRules>
